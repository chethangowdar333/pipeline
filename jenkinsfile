pipeline {
    agent any
    git branch: 'main', url: 'https://github.com/chethangowdar333/pipeline.git'
    stages {
        stage('Build') {
            steps {
                sh 'echo "Hello World"'
                sh '''
                    echo "Multiline shell steps works too"
                    ls -lah
                '''
            }
        }
    }
}
